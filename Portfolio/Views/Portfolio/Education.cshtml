
@{
    ViewBag.Title = "Education";
}
<div>
    <a href="/">Home</a>
    <a href="/Portfolio/ProfileControler">Profile</a>
    <a href="/Portfolio/Education">Education</a>
    <a href="/Portfolio/Projects">Projects</a>
    <a href="/Portfolio/References">References</a>
</div>
<h2>Education</h2>

<ul>
    @foreach (var item in ViewBag.Education)
    {
        <li>
            @item.Degree
            <ul>
                <li>@item.Institution</li>
                <li>@item.Year</li>
            </ul>
        </li>
    }
</ul>

